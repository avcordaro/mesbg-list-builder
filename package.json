{
  "name": "mesbg",
  "version": "6.17.7",
  "homepage": "https://avcordaro.github.io/mesbg-list-builder/",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc -b && vite build",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "npm run prettier && npm run lint",
    "check:fix": "npm run prettier:fix && npm run lint:fix",
    "version": "sh changelog-version-update.sh %npm_package_version%",
    "postversion": "git push && git push --tags && npm run deploy",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hello-pangea/dnd": "^17.0.0",
    "@mui/icons-material": "^6.0.2",
    "@mui/material": "^6.0.2",
    "chart.js": "^4.4.6",
    "fast-deep-equal": "^3.1.3",
    "file-save": "^0.2.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "jszip": "^3.10.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-numeric-input": "^2.2.3",
    "uuid": "^10.0.0",
    "web-vitals": "^4.2.3",
    "zipson": "^0.2.12",
    "zundo": "^2.2.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.2"
  }
